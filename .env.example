# Ollama Configuration
OLLAMA_HOSTNAME=ollama-server  # Customizable hostname
OLLAMA_IP=192.168.1.XXX        # Replace with your Ollama server IP
OLLAMA_HOST=http://${OLLAMA_HOSTNAME}:11434

# Jira Configuration
JIRA_SERVER=https://your-domain.atlassian.net
JIRA_USER=your@email.com
JIRA_TOKEN=your_api_token

# LLM Provider (options: deepseek, openai, llama)
LLM_PROVIDER=deepseek

# For OpenAI provider, set your API key:
OPENAI_API_KEY=your_openai_api_key
OPEN_AI_MODEL = 'gpt-4'


DRY_RUN=false